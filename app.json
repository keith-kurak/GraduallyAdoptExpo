{
  "expo": {
    "slug": "graduallyadoptexpo",
    "name": "GraduallyAdoptExpo",
    "version": "1.0.0",
    "owner": "keith-kurak",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.graduallyadoptexpo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "ios": {
      "bundleIdentifier": "org.reactjs.native.example.GraduallyAdoptExpo"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 29
          },
          "ios": {
            "deploymentTarget": "15.0"
          }
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "94374dba-263e-45ae-a5b8-1670afc01865"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/94374dba-263e-45ae-a5b8-1670afc01865"
    },
    "scheme": "graduallyadopt",
    "web": {
      "bundler": "metro"
    }
  }
}
